//If J==0 till then we can do insersion sort || i has to start from 1 instead of 0
#include <stdio.h>
int main()
{
    int arr[] = {15,2,3,9,18,6,3,17,21,12,3,17,19,27};
    int n = sizeof(arr)/sizeof(arr[0]);
    for(int i=1;i<n;i++)
    {
        int key = arr[i];
        int j = i-1;
        while(j>=0 && arr[j]>key)
        {
            arr[j+1] = arr[j];
            j--;
        }
        arr[j+1] = key;
    }
    printf("Sorted array: \n");
    for(int i=0;i<n;i++)
    {
        printf("%d ",arr[i]);
    }
    return 0;
}
