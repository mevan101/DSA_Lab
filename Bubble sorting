#include<stdio.h> // Bubble sort
int main()
{
    int arr[] = {15,3,14,52,21,57,12,9,19};
    int n = sizeof(arr)/sizeof(arr[0]); // Find the size of any array

    for(int i=0; i<n; i++)
        {
            for(int j=0; j<n-i-1; j++)
            {
                if(arr[j]>arr[j+1])
                {
                    int temp = arr[j];
                    arr[j] = arr[j+1];
                    arr[j+1] = temp;
                }
            }
        }

    for(int i=0; i<n; i++)
    {
        printf("%d ->",arr[i]);
    }

    return 0;
}
